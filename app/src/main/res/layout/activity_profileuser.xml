<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context="com.example.acer.myapplication.profileuser">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="278dp"
        android:layout_height="692dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/pro"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="57dp" />

    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageView
        android:layout_width="198dp"
        android:layout_height="114dp"
        android:background="@drawable/pro2"
        tools:layout_editor_absoluteY="57dp"
        tools:layout_editor_absoluteX="176dp"
        android:id="@+id/imageView5"
        android:layout_below="@+id/imageView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="68dp"
        android:background="@drawable/nn"
        tools:layout_editor_absoluteY="230dp"
        tools:layout_editor_absoluteX="316dp"
        android:layout_height="68dp"
        android:id="@+id/imageView8"
        android:layout_marginBottom="27dp"
        android:layout_above="@+id/imageView12"
        android:layout_alignParentEnd="true" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/birth"
        android:id="@+id/imageView12"
        android:layout_above="@+id/imageView9"
        android:layout_alignStart="@+id/imageView9" />


    <ImageView
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:background="@drawable/msg"
        tools:layout_editor_absoluteY="367dp"
        tools:layout_editor_absoluteX="312dp"
        android:id="@+id/imageView9"
        android:layout_above="@+id/logoutBtn1"
        android:layout_alignParentEnd="true" />


    <ImageView
        android:layout_width="68dp"
        android:background="@drawable/usr"
        tools:layout_editor_absoluteY="298dp"
        tools:layout_editor_absoluteX="312dp"
        android:id="@+id/imageView7"
        android:layout_height="68dp"
        android:layout_above="@+id/imageView9"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="65dp" />

    <ImageButton
        android:layout_width="240dp"
        android:layout_height="42dp"
        tools:layout_editor_absoluteY="445dp"
        tools:layout_editor_absoluteX="128dp"
        android:scaleType="fitXY"
        android:background="@drawable/exit"
        android:id="@+id/logoutBtn1"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="31dp"
        android:layout_above="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton" />

    <ImageButton
        android:layout_width="240dp"
        android:layout_height="42dp"
        tools:layout_editor_absoluteY="469dp"
        tools:layout_editor_absoluteX="128dp"
        android:id="@+id/imageButton"
        android:scaleType="fitXY"
        android:elevation="0dp"
        android:background="@drawable/delete"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:onClick="deleteAccount"/>

    <ListView
        android:id="@+id/listV"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toStartOf="@+id/imageView5"
        android:layout_above="@+id/logoutBtn1"
        android:layout_below="@+id/imageView5" />

    <ImageButton
        android:onClick="homee"
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/kshk"
        app:srcCompat="@drawable/kshk" />

</RelativeLayout>

