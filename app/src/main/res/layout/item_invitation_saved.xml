<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_invitatonssaved"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="1100sp"
            android:layout_height="50sp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:background="@drawable/picture3"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="172dp" />


        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/imageView10"
            android:layout_alignTop="@+id/imageView10"
            android:layout_marginRight="20dp"
            android:layout_marginTop="16dp"
            android:text="@string/graduation_ceremony"
            android:textColor="#7a5a23" />


        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="65dp"
            android:layout_height="55dp"
            android:layout_alignBottom="@+id/imageButton8"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/imageButton8"
            android:background="@drawable/send"
            android:onClick="frindslist"
            android:scaleType="fitXY"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="185dp" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="65dp"
            android:layout_height="55dp"
            android:layout_alignBottom="@+id/imageView10"
            android:layout_alignTop="@+id/imageButton7"
            android:layout_toEndOf="@+id/imageButton12"
            android:layout_toRightOf="@+id/imageButton12"
            android:background="@drawable/edit"
            android:scaleType="fitXY"
            tools:layout_editor_absoluteX="71dp"
            tools:layout_editor_absoluteY="185dp" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="65dp"
            android:layout_height="55dp"
            android:layout_alignBottom="@+id/imageView10"
            android:layout_alignTop="@+id/imageView10"
            android:layout_toEndOf="@+id/imageButton8"
            android:layout_toRightOf="@+id/imageButton8"
            android:background="@drawable/cancel"
            android:scaleType="fitXY"
            tools:layout_editor_absoluteX="142dp"
            tools:layout_editor_absoluteY="185dp" />
</RelativeLayout>

