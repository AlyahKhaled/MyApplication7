<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/background"
    tools:context="com.example.acer.myapplication.admienprofile">

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/kshk"
        android:onClick="homee"
        app:srcCompat="@drawable/kshk" />

    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageView
        android:layout_width="278dp"
        android:layout_height="692dp"
        android:background="@drawable/pro"
        tools:layout_editor_absoluteY="57dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageView4"
        android:layout_below="@+id/imageView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="68dp"
        android:background="@drawable/usr"
        tools:layout_editor_absoluteY="298dp"
        tools:layout_editor_absoluteX="312dp"
        android:id="@+id/imageView7"
        android:layout_height="68dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="84dp" />

    <ImageView
        android:layout_width="68dp"
        android:background="@drawable/nn"
        tools:layout_editor_absoluteY="230dp"
        tools:layout_editor_absoluteX="316dp"
        android:layout_height="68dp"
        android:layout_above="@+id/imageView7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageView8" />


    <ImageView
        android:layout_width="158dp"
        android:layout_height="153dp"
        tools:layout_editor_absoluteY="69dp"
        tools:layout_editor_absoluteX="80dp"
        android:id="@+id/imageView5"
        android:background="@drawable/profile"
        android:layout_below="@+id/imageView"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginTop="19dp" />

    <ImageButton
        android:id="@+id/logoutBtn"
        android:layout_width="240dp"
        android:layout_height="42dp"
        tools:layout_editor_absoluteY="445dp"
        tools:layout_editor_absoluteX="128dp"
        android:scaleType="fitXY"
        android:background="@drawable/exit"
        android:layout_marginBottom="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="13dp"
        android:layout_marginEnd="13dp" />

    <ListView
        android:id="@+id/listV1"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_above="@+id/imageView7"
        android:layout_alignParentStart="true" />

</RelativeLayout>
