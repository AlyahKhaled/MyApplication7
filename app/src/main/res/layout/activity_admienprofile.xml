<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/background"
    tools:context="com.example.acer.myapplication.admienprofile">

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="80dp"
        android:layout_height="200dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/kshk"
        android:onClick="homee"

        android:layout_alignBottom="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="278dp"
        android:layout_height="692dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/pro"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="57dp" />

    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageView
        android:layout_width="68dp"
        android:background="@drawable/usr"
        tools:layout_editor_absoluteY="298dp"
        tools:layout_editor_absoluteX="312dp"
        android:id="@+id/imageView7"
        android:layout_height="68dp"
        android:layout_below="@+id/imageView8"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="21dp" />

    <ImageView
        android:layout_width="68dp"
        android:background="@drawable/nn"
        tools:layout_editor_absoluteY="230dp"
        tools:layout_editor_absoluteX="316dp"
        android:layout_height="68dp"
        android:id="@+id/imageView8"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="29dp" />


    <ImageView
        android:layout_width="158dp"
        android:layout_height="153dp"
        tools:layout_editor_absoluteY="69dp"
        tools:layout_editor_absoluteX="80dp"
        android:id="@+id/imageView5"
        android:background="@drawable/profile"
        android:layout_below="@+id/imageView"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginTop="19dp" />

    <ImageButton
        android:id="@+id/logoutBtn"
        android:layout_width="240dp"
        android:layout_height="42dp"
        tools:layout_editor_absoluteY="445dp"
        tools:layout_editor_absoluteX="128dp"
        android:scaleType="fitXY"
        android:background="@drawable/exit"
        android:layout_marginBottom="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="13dp"
        android:layout_marginEnd="13dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_marginBottom="20dp"
        android:layout_above="@+id/imageView7"
        android:layout_alignEnd="@+id/imageView5"
        android:layout_marginEnd="14dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_alignTop="@+id/imageView7"
        android:layout_alignStart="@+id/textView11"
        android:layout_marginTop="21dp" />

</RelativeLayout>
